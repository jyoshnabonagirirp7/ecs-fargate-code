{
  "family": "bluegreen-taskdef",
  "networkMode": "awsvpc",
  "executionRoleArn": "arn:aws:iam::590157784153:role/ecsTaskExecutionRole",
  "taskRoleArn": "arn:aws:iam::590157784153:role/ecsTaskExecutionRole",
  "containerDefinitions": [
    {
      "name": "ecs-container",
      "image": "<IMAGE_URI>",
      "cpu": 1024,
      "memory": 3072,
      "portMappings": [
        {
          "name": "myport",
          "containerPort": 8080,
          "hostPort": 8080,
          "protocol": "tcp",
          "appProtocol": "http"
        }
      ],
      "essential": true,
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/bluegreen-taskdef",
          "awslogs-region": "us-east-1",
          "awslogs-stream-prefix": "ecs"
        }
      }
    }
  ],
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "cpu": "1024",
  "memory": "3072",
  "runtimePlatform": {
    "cpuArchitecture": "X86_64",
    "operatingSystemFamily": "LINUX"
  }
}
